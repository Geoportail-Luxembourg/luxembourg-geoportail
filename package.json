{
  "name": "luxembourg-geoportail",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "preview": "vite preview",
    "test": "npm run test:unit",
    "test:unit": "vitest --environment jsdom --root .",
    "test:unit:ci": "vitest run --environment jsdom --coverage",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:ci": "start-server-and-test 'vite dev --port 4173' :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:js": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts --ignore-path .gitignore",
    "lint:prettier": "prettier --check . --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:prettier",
    "format": "prettier --write --list-different . && npm run lint:js -- --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@camptocamp/ogc-client": "^0.3.4",
    "ol": "^7.1.0",
    "pinia": "^2.0.26",
    "sortablejs": "^1.15.0",
    "tailwindcss": "^3.2.4",
    "vue": "^3.2.45",
    "vue-dompurify-html": "^3.1.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.9",
    "@types/proj4": "^2.5.2",
    "@types/sortablejs": "^1.15.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.26.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.4",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^11.2.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.2",
    "i18next": "^22.0.6",
    "i18next-http-backend": "^2.0.2",
    "i18next-parser": "^7.0.3",
    "i18next-vue": "^2.1.1",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "proj4": "^2.8.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.7.4",
    "vite": "^3.2.4",
    "vite-plugin-istanbul": "^4.0.0",
    "vitest": "^0.25.3",
    "vue-tsc": "^1.0.9"
  },
  "lint-staged": {
    "*.ts|*.vue": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
